<p id="notice"><%= notice %></p>

<h1>Fotos Obras</h1>

<table>
  <thead>
    <tr>
      <th>Obra</th>
      <th>Foto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fotos_obras.each do |fotos_obra| %>
      <tr>
        <td><%= fotos_obra.obra_id %></td>
        <td><%= fotos_obra.foto %></td>
        <td><%= link_to 'Show', fotos_obra %></td>
        <td><%= link_to 'Edit', edit_fotos_obra_path(fotos_obra) %></td>
        <td><%= link_to 'Destroy', fotos_obra, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fotos Obra', new_fotos_obra_path %>
